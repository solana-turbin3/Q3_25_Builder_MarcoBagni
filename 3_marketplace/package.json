{
  "license": "ISC",
  "type": "module",
  "scripts": {
    "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
    "deploy:local": "anchor deploy --provider.cluster localnet",
    "deploy:devnet": "anchor deploy --provider.cluster devnet",
    "deploy:mainnet": "anchor deploy --provider.cluster mainnet",
    "test:local": "NODE_NO_WARNINGS=1 anchor test --provider.cluster localnet --skip-local-validator --skip-deploy --skip-build",
    "test:devnet": "NODE_NO_WARNINGS=1 anchor test --provider.cluster devnet --skip-local-validator --skip-deploy --skip-build",
    "test:mainnet": "NODE_NO_WARNINGS=1 anchor test  --provider.cluster mainnet --skip-local-validator --skip-deploy --skip-build",
    "admin": "ANCHOR_PROVIDER_URL=https://api.devnet.solana.com yarn ts-node scripts/app-admin.ts",
    "user": "ANCHOR_PROVIDER_URL=https://api.devnet.solana.com yarn ts-node scripts/app-user.ts"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@metaplex-foundation/mpl-candy-machine": "^6.1.0",
    "@metaplex-foundation/mpl-token-metadata": "^3.4.0",
    "@metaplex-foundation/umi": "^1.2.0",
    "@metaplex-foundation/umi-bundle-defaults": "^1.2.0",
    "@metaplex-foundation/umi-signer-wallet-adapters": "^1.2.0",
    "@solana/spl-token": "^0.4.13"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "mocha": "^9.0.3",
    "prettier": "^2.6.2",
    "ts-mocha": "^10.0.0",
    "typescript": "^5.7.3"
  }
}
